<?xml version="1.0" encoding="UTF-8"?>
<jboss-web>
  <context-root>CA</context-root>
  <class-loading java2ClassLoadingCompliance="false">
    <loader-repository>
      webconnect.war:loader=webconnect.war
      <loader-repository-config>java2ParentDelegation=false</loader-repository-config>
    </loader-repository>
  </class-loading>
  <resource-ref>
    <res-ref-name>jdbc/webconnectDatasource</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <jndi-name>java:/webconnectDS</jndi-name>
  </resource-ref>
</jboss-web>

